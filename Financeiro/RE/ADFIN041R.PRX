		cFilialSEA := "EA_FILIAL = '" + xFilial("SEA") + "' "
	EndIf                     			              
		
	cFilialSEA := "%"+cFilialSEA+"%" 

	BeginSql Alias cAliasSea
		SELECT 	SEA.EA_FILIAL, SEA.EA_FILORIG, SEA.EA_NUMBOR, SEA.EA_CART, SEA.EA_PREFIXO, SEA.EA_NUM,
